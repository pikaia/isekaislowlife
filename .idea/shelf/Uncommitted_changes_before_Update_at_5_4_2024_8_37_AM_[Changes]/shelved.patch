+import re
+
+import pytesseract
+from PIL import Image
+
+print(pytesseract.__version__)
+# The following line is for Windows users only. You may need to change the path below depending
+# on the path of your Tesseract OCR installation.
+pytesseract.pytesseract.tesseract_cmd = 'C:\\Program Files\\Tesseract-OCR\\tesseract.exe'
+
+buildings = ['inn', 'apothecary', 'workshop', 'scroll shop', 'spring resort', 'central station',
+             'patisserie', 'archery range', 'clinic', 'market street', 'bank', 'tailor shop',
+             'sports park', 'museum', 'theater']
+
+earnings = {'Earnings': 0.0,
+            'Basic Earnings': 0.0,
+            'Fellow Operation Ability': 0.0,
+            'Total Employee Earnings': 0.0,
+            'Fish1': 0.0,
+            'Earnings Bonus': 0.0,
+            'Operating Fellow': 0.0,
+            'Upgrade': 0.0,
+            'Family Member': 0.0,
+            'Farmstead': 0.0,
+            'Inn': 0.0,
+            'Magic Farm': 0.0,
+            'Fish2': 0.0,
+            'Challenge': 0.0,
+            'Guild': 0.0,
+            }
+
+for bldg in buildings:
+    for ss_type in ['earning', 'operating_fellow']:
+
+        # Convert the image to grayscale
+        log.info(f'Extracting text from resources/ss/data/generated/building/{bldg}_{ss_type}.png...')
+        image = Image.open(f'resources/ss/data/generated/building/{bldg}_{ss_type}.png').convert("L")
+
+        # Perform OCR on the image
+        text = pytesseract.image_to_string(image)
+
+        # Print the extracted text to the console
+        print(text, '\n\n')
+
+types = ['inspiring', 'diligent', 'brave', 'informed', 'unfettered']
+pos = []
+for line in text.splitlines():
+    for typ in types:
+        pattern = re.compile(r'.*{type}.*\+([0-9].*)%', re.IGNORECASE)
+        result = re.search(pattern, line)
+        if result is not None:
+            print(line, result, '||', result.groups(), '||', result.group(result.lastindex))
+            pos[typ] = result.group(result.lastindex)
+
+# Total unlocked skills: 681
+# ‘@ Inspiring Building Earning Bonus +4593%
+# ‘@ Diligent Building Earning Bonus +4776%
+# @Brave Building Earning Bonus +4658%
+#
+# @ Informed Building Earning Bonus +4746%
+# @ Unfettered Building Earning Bonus +4604%
